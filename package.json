{
  "name": "bookstore-ecommerce",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run ng build --prod --build-optimizer --vendor-chunk --common-chunk --service-worker",
    "start": "npm run ng serve --aot --proxy-config=proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "e2e": "npm run ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -t --hideGenerator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.3",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.1.2",
    "@angular/http": "~7.2.15",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@covalent/core": "^2.1.1",
    "@covalent/dynamic-forms": "^2.1.1",
    "@covalent/echarts": "^2.1.0",
    "@covalent/highlight": "^2.1.1",
    "@covalent/http": "^2.1.1",
    "@covalent/markdown": "^2.1.1",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@kolkov/angular-editor": "^1.0.0-beta.4",
    "@nebular/eva-icons": "^4.2.0",
    "@nebular/theme": "^4.2.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "echarts": "^4.4.0",
    "eva-icons": "^1.1.0",
    "hammerjs": "^2.0.8",
    "ionicons": "2.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.24.0",
    "nebular-icons": "1.1.0",
    "ngx-color-picker": "^8.1.0",
    "ngx-cookie-service": "^2.2.0",
    "ngx-logger": "^3.3.4",
    "ngx-toastr": "^10.0.4",
    "pace-js": "^1.0.2",
    "primeicons": "^2.0.0",
    "primeng": "^8.0.2",
    "roboto-fontface": "0.8.0",
